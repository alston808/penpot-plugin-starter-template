{
  "name": "penpot-ai-layout",
  "private": true,
  "version": "1.0.0",
  "description": "AI-powered layout generator for Penpot using OpenRouter",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "build:production": "NODE_ENV=production bun run build",
    "preview": "vite preview",
    "deploy": "bun run build && wrangler pages deploy dist"
  },
  "dependencies": {
    "@penpot/plugin-styles": "1.3.2",
    "@penpot/plugin-types": "1.3.2"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-live-preview": "^0.3.2",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
